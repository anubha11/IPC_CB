//find pairs with sum of two numbers equal to target

#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
    int n,target,i,j;
    cin>>n;
    int arr[n];
    for(i=0; i<n;i++){
        cin>>arr[i];
    }
    cin>>target;
    sort(arr,arr+n);
    i=0;
    j=n-1;
    while(i<=j){
        int sum=arr[i]+arr[j];
        if (target==sum){
            cout<<arr[i]<<" and "<<arr[j]<<endl;
            i++;
            j--;
        }else if(target>sum){
            i++;
        }else if(target<sum){
            j--;
        }
    }
    return 0;
}
