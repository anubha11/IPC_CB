// find two distinct numbers in an array of the form 2n+2

#include<iostream>
using namespace std;

int main() {
	int len,all_xor=0;
	cin>>len;
	int* arr=new int[len];
	for(int i=0;i<len; i++){
		cin>>arr[i];
		all_xor=all_xor^arr[i];
	}
	int tempxor=all_xor,d=0;
    while(tempxor){
        if(tempxor&1){
            break;
        }
        tempxor>>=1;
        d++;
    }
    int ans1 = 0;
	int mask = (1<<d);
	for(int i=0;i<len;i++){
		if((arr[i]&mask) >0){
			ans1 ^= arr[i];
		}
    }
    int ans2=ans1^all_xor;
    cout<<ans1<<" "<<ans2;
	return 0;
}
