//find the unique number from a list of numbers
//array is in the form of 2n+1

#include<iostream>
using namespace std;

int main() {
	int len,all_xor=0;
	cin>>len;
	int* arr=new int[len];
	for(int i=0;i<len; i++){
		cin>>arr[i];
		all_xor=all_xor^arr[i];
        cout<<arr[i]<<" "<<all_xor<<endl;
	}
	cout<<all_xor;

	return 0;
}
